		$(document).ready(function(){
                if($("#success").length){
                Swal.fire($("#success").text(), "", "success");  
            }
                if($("#error").length){
                Swal.fire($("#error").text(), "", "error");  
            }
                $("#month").datepicker( {
                format: "yyyy-mm",
                startView: "months", 
                minViewMode: "months",
                autoclose: true
            });
            $(".amountd").each(function(i) {
            $(this).mask('000,000,000', {reverse: true}); 		});
		});

        "use strict";

        // Shared Colors Definition
        const primary = '#6993FF';
        const success = '#1BC5BD';
        const info = '#8950FC';
        const warning = '#FFA800';
        const danger = '#F64E60';

        let x = new Array(); 
        let count = new Array();

        let months = document.getElementsByName("date");
        let counts = document.getElementsByName("count");

        for(var i = 0; i<months.length; i++){
            x.push(months[i].value)
            count.push(counts[i].value);
        }
        
        console.log(x);
        console.log(count);

        var KTApexChartsDemo = function () {
            // Private functions
            var _demo1 = function () {
                const apexChart = "#chart_1";
                var options = {
                    series: [{
                        name: "Хэрэглэгчийн тоо",
                        data: count
                    }],
                    chart: {
                        height: 350,
                        type: 'line',
                        zoom: {
