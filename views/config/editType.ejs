        $("#submits").click(function(){
            
            var number = $("#accountNumber").val()
            var name = $("#accountName").val()
            var currency = $("#currency").val()
            if (!number.trim() || number.length !=10 || number.isNumeric) {
                $("#accountNumber").addClass('is-invalid');
                if(!$("#numberError").length){
                    $("#accountNumber").after('<div class="invalid-feedback" id="numberError">Дансны дугаар 10 оронтой бөгөөд зөвхөн тооноос бүрдэнэ.</div>')
                }
                return false;
            }
            if(!name){
                $("#accountName").addClass('is-invalid')
